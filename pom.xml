	<version>1.2.0</version>
	<packaging>pom</packaging>
	<name>Oms-parent</name>
	<description>Oms-parent</description>

	<modules>
		<module>Oms-base-api</module>
		<module>Oms-pojo</module>
		<module>Oms-utils</module>
