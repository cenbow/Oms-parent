	<dependencyManagement>
		<dependencies>

            <dependency>
                <groupId>com.work.shop.oms</groupId>
                <artifactId>Oms-base-api</artifactId>
                <version>1.4.2</version>
            </dependency>

            <dependency>
                <groupId>com.alibaba</groupId>
